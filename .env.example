# PICO-8 Configuration
# Path to PICO-8 executable (required)
PICO8_PATH=/Applications/pico-8/PICO-8.app/Contents/MacOS/pico8

# Optional: Launch PICO-8 in windowed mode (default: true)
PICO8_WINDOWED=true

# Optional: Set PICO-8 sound volume 0-256 (default: 128)
PICO8_VOLUME=128

# Optional: Default cartridge to load
PICO8_DEFAULT_CARTRIDGE=input/cartridges/test_game.p8

# Application Configuration
# Optional: Enable debug mode
APP_DEBUG=false